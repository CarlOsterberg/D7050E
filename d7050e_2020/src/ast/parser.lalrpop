use std::str::FromStr;

use crate::ast::*;

grammar;

pub NumOrId: NumOrId = {
    Num => NumOrId::Num(<>),
    Id => NumOrId::Id(<>),
}
 
pub Num: usize = {
    r"[0-9]+" => usize::from_str(<>).unwrap(),
};

pub Id: String = {
    r"([a-z]|[A-Z])([a-z]|[A-Z]|[0-9]|_)*" => String::from_str(<>).unwrap(),
};
